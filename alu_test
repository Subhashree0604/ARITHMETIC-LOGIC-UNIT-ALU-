#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_014747C8 .scope module, "tb_ALU" "tb_ALU" 2 3;
 .timescale -9 -12;
v01478FF8_0 .var "A", 3 0;
v01479050_0 .net "ALU_Out", 3 0, v01473340_0; 1 drivers
v014790A8_0 .var "ALU_Sel", 2 0;
v0147AA10_0 .var "B", 3 0;
S_01473260 .scope module, "uut" "ALU" 2 10, 3 1, S_014747C8;
 .timescale 0 0;
v014732E8_0 .net "A", 3 0, v01478FF8_0; 1 drivers
v01473340_0 .var "ALU_Out", 3 0;
v01478F48_0 .net "ALU_Sel", 2 0, v014790A8_0; 1 drivers
v01478FA0_0 .net "B", 3 0, v0147AA10_0; 1 drivers
E_01478008 .event edge, v01478F48_0, v014732E8_0, v01478FA0_0;
    .scope S_01473260;
T_0 ;
    %wait E_01478008;
    %load/v 8, v01478F48_0, 3;
    %cmpi/u 8, 0, 3;
    %jmp/1 T_0.0, 6;
    %cmpi/u 8, 1, 3;
    %jmp/1 T_0.1, 6;
    %cmpi/u 8, 2, 3;
    %jmp/1 T_0.2, 6;
    %cmpi/u 8, 3, 3;
    %jmp/1 T_0.3, 6;
    %cmpi/u 8, 4, 3;
    %jmp/1 T_0.4, 6;
    %set/v v01473340_0, 0, 4;
    %jmp T_0.6;
T_0.0 ;
    %load/v 8, v014732E8_0, 4;
    %load/v 12, v01478FA0_0, 4;
    %add 8, 12, 4;
    %set/v v01473340_0, 8, 4;
    %jmp T_0.6;
T_0.1 ;
    %load/v 8, v014732E8_0, 4;
    %load/v 12, v01478FA0_0, 4;
    %sub 8, 12, 4;
    %set/v v01473340_0, 8, 4;
    %jmp T_0.6;
T_0.2 ;
    %load/v 8, v014732E8_0, 4;
    %load/v 12, v01478FA0_0, 4;
    %and 8, 12, 4;
    %set/v v01473340_0, 8, 4;
    %jmp T_0.6;
T_0.3 ;
    %load/v 8, v014732E8_0, 4;
    %load/v 12, v01478FA0_0, 4;
    %or 8, 12, 4;
    %set/v v01473340_0, 8, 4;
    %jmp T_0.6;
T_0.4 ;
    %load/v 8, v014732E8_0, 4;
    %inv 8, 4;
    %set/v v01473340_0, 8, 4;
    %jmp T_0.6;
T_0.6 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_014747C8;
T_1 ;
    %vpi_call 2 18 "$display", "Starting ALU Test...";
    %movi 8, 3, 4;
    %set/v v01478FF8_0, 8, 4;
    %movi 8, 1, 4;
    %set/v v0147AA10_0, 8, 4;
    %set/v v014790A8_0, 0, 3;
    %delay 10000, 0;
    %vpi_call 2 22 "$display", "ADD: %b + %b = %b", v01478FF8_0, v0147AA10_0, v01479050_0;
    %movi 8, 5, 4;
    %set/v v01478FF8_0, 8, 4;
    %movi 8, 1, 4;
    %set/v v0147AA10_0, 8, 4;
    %movi 8, 1, 3;
    %set/v v014790A8_0, 8, 3;
    %delay 10000, 0;
    %vpi_call 2 26 "$display", "SUB: %b - %b = %b", v01478FF8_0, v0147AA10_0, v01479050_0;
    %movi 8, 12, 4;
    %set/v v01478FF8_0, 8, 4;
    %movi 8, 10, 4;
    %set/v v0147AA10_0, 8, 4;
    %movi 8, 2, 3;
    %set/v v014790A8_0, 8, 3;
    %delay 10000, 0;
    %vpi_call 2 30 "$display", "AND: %b & %b = %b", v01478FF8_0, v0147AA10_0, v01479050_0;
    %movi 8, 12, 4;
    %set/v v01478FF8_0, 8, 4;
    %movi 8, 10, 4;
    %set/v v0147AA10_0, 8, 4;
    %movi 8, 3, 3;
    %set/v v014790A8_0, 8, 3;
    %delay 10000, 0;
    %vpi_call 2 34 "$display", "OR: %b | %b = %b", v01478FF8_0, v0147AA10_0, v01479050_0;
    %movi 8, 12, 4;
    %set/v v01478FF8_0, 8, 4;
    %set/v v0147AA10_0, 0, 4;
    %movi 8, 4, 3;
    %set/v v014790A8_0, 8, 3;
    %delay 10000, 0;
    %vpi_call 2 38 "$display", "NOT: ~%b = %b", v01478FF8_0, v01479050_0;
    %vpi_call 2 40 "$display", "ALU Test Complete.";
    %vpi_call 2 41 "$finish";
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
